# Getting Proxy Template
wget -q -O /var/kagutom https://raw.githubusercontent.com/lexiscript69/lexisnds/main/gutom
chmod +x /var/kagutom

function service1() {
# Installing Service
cat << END > /etc/systemd/system/kagutom.service  
[Unit]
Description=supotka
Documentation=https://google.com
After=network.target nss-lookup.target

[Service]
Type=simple
User=root
CapabilityBoundingSet=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
AmbientCapabilities=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
NoNewPrivileges=true
ExecStart=/usr/bin/python -O /var/kagutom
Restart=on-failure

[Install]
WantedBy=multi-user.target
END
}
service1

function setting() {
sed -i "/DEFAULT_HOST = '127.0.0.1:443'/c\DEFAULT_HOST = '127.0.0.1:550'" /var/kagutom

systemctl daemon-reload
systemctl enable kagutom
systemctl restart kagutom
}
setting

function cron22() {
echo -e "0 3 * * * /sbin/reboot >/dev/null 2>&1" >> mycron
     echo -e "*/30 * * * * sudo service kagutom restart" >> mycron
          crontab mycron
     service cron restart

}
cron22

sed -i "s|https://openvpn.net/wp-content/uploads/openvpn.jpg|https://github.com/kagutom/gutom/raw/main/20221204_193101_0000.png|g" /var/www/openvpn/index.html && service nginx restart

clear

history -c

echo "NAKAKA GUTOM KUMAIN"
